{
    "name": "base",
    "data": {
        "index_path": "/kaggle/working/stylegan",
        "data_path": "/kaggle/input/anime-faces/data/data",
        "parts": {
            "train": {
                "batch_size": 32,
                "limit": 19488
            },
            "val": {
                "batch_size": 32,
                "limit": 1024
            },
            "test": {
                "batch_size": 32,
                "limit": 1024
            }
        }
        
    },
    "arch": {
        "Generator": {
            "args": {
                "channels": [100, 1024, 512, 256, 128, 3],
                "strides": [1, 2, 2, 2, 2],
                "kernel_sizes": [4, 4, 4, 4, 4],
                "paddings": [0, 1, 1, 1, 1]
            }
        },
        "Discriminator": {
            "args": {
                "channels": [3, 128, 256, 512, 1024, 1],
                "strides": [2, 2, 2, 2, 4],
                "kernel_sizes": [5, 5, 5, 5, 5],
                "paddings": [2, 2, 2, 2, 1],
                "leaky_slope": 0.2
            }
        }
    },
    "optimizer": {
        "name": "Adam",
        "args": {
            "lr": 0.0002,
            "betas": [0.5, 0.999]
        }
    },
    "trainer": {
        "len_epoch": 609,
        "log_period": 10,
        "num_epochs": 1000
    }
}